PREFIX : <http://bigdata.com>
PREFIX foaf: <http://xmlns.com/foaf/0.1/>
PREFIX dct:  <http://purl.org/dc/elements/1.1/>

CONSTRUCT {
  ?bob foaf:name "Bob" .
  <<?bob foaf:age ?age>> dct:creator <http://example.com/crawlers#c1>;
                         dct:source ?src .
}
WHERE {
  ?bob foaf:name "Bob" .
  <<?bob foaf:age ?age>> dct:source ?src .
}
