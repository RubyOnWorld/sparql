:ppmm02 rdf:type mf:QueryEvaluationTest ;
    mf:name    "(pp02) Star path (min/max)" ;
    dawgt:approval dawgt:Approved ;
    dawgt:approvedBy <http://www.w3.org/2009/sparql/meeting/2011-03-29#resolution_3> ;
    mf:action
         [ qt:query  <ppmm02.rq> ;
           qt:data   <pp01.ttl> ] ;
    mf:result  <pp02.srx>
    .
    
